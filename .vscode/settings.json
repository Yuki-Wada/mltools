    "python.linting.pylintArgs": [
        "--disable=C0103",
        "--extension-pkg-whitelist=numpy, torch",
        "--generated-members=numpy.*",
        "--generated-members=torch.*",
        "--generated-members=tensorflow.*",
        "--ignored-modules=tensorflow.keras",
        "--ignored-modules=torch",
        "--not-callable=torch.*",
